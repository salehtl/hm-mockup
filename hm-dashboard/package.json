  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:json-server": "VITE_USE_JSON_SERVER=true concurrently \"json-server --watch data/db.json --port 3001\" \"vite --host\"",
